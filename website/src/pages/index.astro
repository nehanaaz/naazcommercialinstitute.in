---
import HomeHero from "@/components/home/hero";
import HomeMission from "@/components/home/mission";
import HomeImpact from "@/components/home/impact";
import HomeGallery from "@/components/home/gallery";
import HomeDonors from "@/components/home/donors";
import FAQSection from "@/components/faqs";
import Page from "@/layouts/page.astro";
---

<Page>
  <main id="main-content" role="main">
    <HomeHero client:load />
    <HomeMission client:load />
    <HomeImpact client:load />
    <HomeGallery client:load />
    <HomeDonors client:load />
    <FAQSection client:load limit={4} showAllLink={true} />
  </main>
</Page>
